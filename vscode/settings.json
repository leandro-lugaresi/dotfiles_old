{
  "editor.rulers": [
    80,
    120
  ],
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.trimAutoWhitespace": true,
  "editor.detectIndentation": false,
  "editor.tabCompletion": "on",
  "editor.renderWhitespace": "boundary",
  "editor.cursorBlinking": "blink",
  "editor.minimap.enabled": true,
  "editor.fontWeight": "300",
  "editor.fontFamily": "'Cascadia Code Regular','Fira Code','Anonymous Pro'",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "explorer.openEditors.visible": 0,
  "explorer.confirmDragAndDrop": false,
  "window.restoreWindows": "none",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": -1,
  "files.autoSave": "off",
  "files.trimTrailingWhitespace": true,
  "go.coverOnSave": true,
  "go.testOnSave": true,
  "go.formatTool": "gofmt",
  "go.coverageDecorator": {
    "type": "gutter"
  },
  "go.formatFlags": ["-s", "-w"],
  "go.buildFlags": ["-v"],
  "go.lintTool": "golangci-lint",
  "go.docsTool": "guru",
  "go.useLanguageServer": true,
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true, // for build and vet errors as you type
    "findReferences": true // for finding in-package references
  },
  "gopls": {
    "usePlaceholders": true // add parameter placeholders when completing a function
  },
  "files.eol": "\n",
  "workbench.editor.showIcons": true,
  "workbench.startupEditor": "none",
  "extensions.ignoreRecommendations": true,
  "extensions.autoUpdate": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "/vendor": true
  },
  "docker.showExplorer": false,
  "terminal.integrated.fontFamily": "'Hack Nerd Font','Anonymous Pro','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "terminal.external.osxExec": "iTerm.app",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "vim.handleKeys": {
    "<C-d>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-f>": false,
    "<C-b>": false,
    "<C-r>": false,
    "<C-w>": false,
    "<C-x>": false
  },
  "vim.easymotion": true,
  "[go]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.snippetSuggestions": "bottom",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[json]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
  },
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
  },
  "gitlens.keymap": "alternate",
  "diffEditor.ignoreTrimWhitespace": false,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true,
    "suppressShowKeyBindingsNotice": true
  },
  "explorer.confirmDelete": false,
  "workbench.sideBar.location": "left",
  "breadcrumbs.enabled": true,
  "terminal.integrated.setLocaleVariables": true,
  "vim.sneak": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "terminal.integrated.rendererType": "dom",
  "cSpell.userWords": [
    "ECONFLICT",
    "EINTERNAL",
    "EINVALID",
    "ENOTFOUND",
    "ULID",
    "empregoligado",
    "healthcheck",
    "kiko",
    "oklog",
    "whatsapp",
    "xerrors"
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "ruby",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "html.format.endWithNewline": true,
  "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.renderControlCharacters": false,
